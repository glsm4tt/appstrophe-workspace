{
  "name": "appstrophe-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "emulators": "firebase emulators:start --import='./test/ressources' --export-on-exit='./test/ressources'",
    "test:e2e:all": "firebase emulators:exec --import='./test/ressources' 'npm run e2e:all'",
    "e2e:all": "npx nx run-many --target=e2e --all --nx-bail --parallel=false",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build:all": "npx nx run-many --target=build --all",
    "test:all": "npx nx run-many --target=test --all --watch=false --nx-bail"
  },
