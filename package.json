  "scripts": {
    "ng": "nx",
    "emulators": "firebase emulators:start --import='./test/ressources'",
    "e2e": "firebase emulators:exec --import='./test/ressources' 'npm run e2e:all'",
    "e2e:all": "npx nx run blog-e2e:e2e --chrome && npx nx run auth-e2e:e2e --chrome && npx nx run shell-e2e:e2e --chrome",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "build": "npx nx run-many --target=build --all",
    "test": "npx nx run-many --target=test --all --watch=false --nx-bail"
  },
